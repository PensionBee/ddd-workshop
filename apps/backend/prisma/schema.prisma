// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model users {
  id         Int     @id @default(autoincrement())
  email      String  @unique
  password   String
  is_active  Boolean @default(true)
  first_name String?
  last_name  String?
}
