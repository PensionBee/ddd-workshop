// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model user {
  id         String  @unique
  email      String  @unique
  password   String
  first_name String
  last_name  String?

  @@index([id])
  @@map("users")
}
